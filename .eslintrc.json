{
    "extends": "airbnb",
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    },
    "globals": {
        "document": false,
        "window": false,
        "navigator": false,
        "MediaRecorder": false,
        "Blob": false,
        "requestAnimationFrame": false,
        "webkitAudioContext": false
    }
}
